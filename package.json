{
  "name": "cocktailer",
  "version": "1.0.0",
  "description": "cocktail recipes app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "develop": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "scss": "node-sass --output-style -o dist/css src/scss",
    "autoprefixer": "postcss -u autoprefixer -r dist/css/*",
    "build:css": "npm run scss && npm run autoprefixer",
    "watch:css": "onchange \"src/scss/*.scss\" -- npm run build:css",
    "watch:all": "concurrently --kill-others \"npm run watch:css\" \"npm run develop\" "
  },
  "author": "Patryk Kilian",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "node-sass": "^4.9.0",
    "onchange": "^4.0.0",
    "postcss-cli": "^5.0.0",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^3.5.1",
    "uniqid": "^4.1.1"
  }
}
